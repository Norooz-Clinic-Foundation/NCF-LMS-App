{
  "expo": {
    "name": "NCF App",
    "slug": "ncf-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.googleusercontent.apps.39709221966-us5m6f6fn75fhke5qba9vjmc98msrbkj",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ncfapp"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
